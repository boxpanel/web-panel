{
  "name": "web-panel",
  "version": "1.0.0",
  "description": "A comprehensive web-based system administration panel",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "system-administration",
    "web-panel",
    "process-management",
    "file-management",
    "system-monitoring"
  ],
  "author": "Web Panel Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "mime-types": "^2.1.35",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2",
    "node-cron": "^3.0.3",
    "systeminformation": "^5.21.20"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/web-panel.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/web-panel/issues"
  },
  "homepage": "https://github.com/your-username/web-panel#readme"
}